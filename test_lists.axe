; Test list functionality

; Create a simple list
(let my_list (list 1 2 3 4 5))
(print "My list:" my_list)

; Get the length
(print "Length:" (len my_list))

; Get elements by index
(print "First element:" (get my_list 0))
(print "Third element:" (get my_list 2))
(print "Last element:" (get my_list -1))

; Push elements
(let new_list (push my_list 6))
(print "After push:" new_list)

; Nested lists
(let nested (list (list 1 2) (list 3 4) (list 5 6)))
(print "Nested list:" nested)

; List with mixed types
(let mixed (list 1 "hello" true null 3.14))
(print "Mixed types:" mixed)
(print "Type of list:" (type mixed))
(print "Type of element:" (type (get mixed 1)))

; Empty list
(let empty (list))
(print "Empty list:" empty)
(print "Empty length:" (len empty))

; List with variables
(let x 10)
(let y 20)
(let var_list (list x y (+ x y)))
(print "List with vars:" var_list)
