// Fibonacci-style calculation (without loops/functions)
// Demonstrates arithmetic and variable assignment

let a = 0;
let b = 1;

// Manual fibonacci steps
let temp = a + b;  // 1
a = b;
b = temp;

temp = a + b;      // 2
a = b;
b = temp;

temp = a + b;      // 3
a = b;
b = temp;

temp = a + b;      // 5
a = b;
b = temp;

temp = a + b;      // 8
a = b;
b = temp;

// b now contains fib(5) = 8
